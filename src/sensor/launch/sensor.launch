<launch> 
    <arg name="fcu_url" default="/dev/ttyACM0:921600"/>
    <arg name="gcs_url" default="udp://@192.168.50.254"/>
    <arg name="SYS_ID" default="5"/>
    <group ns="platform">
        <param name="UAV_ID" value="$(arg SYS_ID)"/>
        <include file="$(find mavros)/launch/px4.launch">
            <arg name="fcu_url" value="$(arg fcu_url)"/>
            <arg name="gcs_url" value="$(arg gcs_url)"/>
            <arg name="tgt_system" value="$(arg SYS_ID)"/>
        </include>
       <node name="topic_tool" pkg="sensor" type="topic_tool_fcu"></node>
       <node name="origin_py" pkg="sensor" type="origin.py"></node>
    </group>
</launch>
